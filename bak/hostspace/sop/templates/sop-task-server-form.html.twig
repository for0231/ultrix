{{form.form_build_id}}
{{form.form_token}}
{{form.form_id}}
<div class="row">
  <div class="col-xs-9">
    <div class="row">
      <div class="col-xs-12">
        <div class="row">
          <div class="col-xs-4">
            {{ form.mips }}
          </div>
          <div class="col-xs-4">
            {{ form.client_uid }}
          </div>
          <div class="col-xs-4">
            {{ form.sop_type }}
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-xs-4">
        <div class="row">
            <div class="col-xs-12">
              {{ form.edit_info.business_ip.bips }}
            </div>
        </div>
      </div>
      <div class="col-xs-8">
        <div class="row">
          <div class="col-xs-12">
            {{ form.pid }}
          </div>
        </div>
        <div class="row">
          <div class="col-xs-12">
            {{ form.bip_type }}
          </div>
        </div>
        <div class="row">
          <div class="col-xs-12">
            {{ form.os }}
          </div>
        </div>
        <div class="row">
          <div class="col-xs-12">
            {{ form.bandwidth }}
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-xs-12">
        <div class="row">
          <div class="col-xs-4">
            {{ form.edit_info.business_ip.ipb_search.ipb_search_wrapper }}
          </div>
          <div class="col-xs-8">
            <div class="row">
              <div class="col-xs-9">
                {{ form.edit_info.business_ip.ipb_search.ipb_search_text }}
              </div>
              <div class="col-xs-3">
                <br/>
                <br/>
                {{ form.edit_info.business_ip.ipb_search.ipb_search_submit }}
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="col-xs-3">
    <div class="row">
      <div class="col-xs-11">
        {{ form.description }}
      </div>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-xs-12 col-md-offset-5">
    {{ form.actions }}
  </div>
</div>
