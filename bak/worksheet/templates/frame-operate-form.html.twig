{#
/**
 * @file
 * 处理上下架工单表单
 *
 * Available variables:
 * - form: 处理上下架工单表单
 *   - client: 公司名称
 *
 */
#}
{{attach_library('worksheet/drupal.worksheet-default') }}
{{form.form_build_id}}
{{form.form_token}}
{{form.form_id}}
{{form.other}}
{{form.js}}
<table class='edit'>
  <tr>
    <td class='col1'>{{form.code}}</td>
    <td class='col1'>{{form.manage_ip}}</td>
    <td>{{form.client}}</td>
  </tr>
  <tr>
    <td>{{form.contacts}}</td>
    <td>{{form.show_created}}</td>
    <td>{{form.show_user}}</td>
  </tr>
  <tr>
    <td rowspan="4" class="business-ip">{{form.business_ip}}</td>
    <td>{{form.show_handleuser}}</td>
    <td>{{form.show_lastuser}}</td>
  </tr>
  <tr>
    <td>{{form.tid}}</td>
    <td>{{form.ip_class}}</td>
  </tr>
  <tr>
    <td>{{form.system}}</td>
    <td>{{form.broadband}}</td>
  </tr>
  <tr>
    <td>{{form.product_name}}</td>
    <td>&nbsp;</td>
  </tr>
  <tr>
    <td class='frame-textarea'>{{form.requirement}}</td>
    <td class='frame-textarea'>{{form.handle_info}}</td>
    <td>
      {{form.problem_difficulty}}
      {{form.add_card}}
      {{form.add_arp}}
    </td>
  </tr>
</table>
{{form.deliver_info_show}}
{{form.actions}}