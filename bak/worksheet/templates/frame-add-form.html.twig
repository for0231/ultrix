{#
/**
 * @file
 * 创建上下架工单表单
 *
 * Available variables:
 * - form: 创建上下架工单表单
 *   - client: 公司名称
 *
 */
#}
{{attach_library('worksheet/drupal.worksheet-default') }}
{{attach_library('worksheet/drupal.worksheet-frame') }}
{{form.form_build_id}}
{{form.form_token}}
{{form.form_id}}
<table class='edit'>
  <tr>
    <td class='col1'>{{form.code}}</td>
    <td class='col1'>{{form.manage_ip}}</td>
    <td>{{form.client}}</td>
  </tr>
  <tr>
    <td rowspan="3" class="business-ip">{{form.business_ip}}</td>
    <td>{{form.tid}}</td>
    <td>{{form.contacts}}</td>
  </tr>
  <tr>
    <td>{{form.ip_class}}</td>
    <td>{{form.system}}</td>
  </tr>
  <tr>
    <td>{{form.broadband}}</td>
    <td>{{form.product_name}}</td>
  </tr>
  <tr>
    <td colspan="3" class='frame-add'>{{form.requirement}}</td>
  </tr>
</table>
{{form.actions}}
