{#
/**
 * @file
 * 创建机房工单表单
 *
 * Available variables:
 * - form: 创建机房工单表单
 *   - client: 公司名称
 *
 */
#}
{{attach_library('worksheet/drupal.worksheet-default') }}
{{form.form_build_id}}
{{form.form_token}}
{{form.form_id}}
<table class='edit'>
  <tr>
    <td class='col1'>{{form.code}}</td>
    <td class='col1'>{{form.manage_ip}}</td>
    <td>{{form.client}}</td>
  </tr>
  <tr>
    <td>{{form.contacts}}</td>
    <td>{{form.cabinet}}</td>
    <td>{{form.port}}</td>
  </tr>
  <tr>
    <td>{{form.product_name}}</td>
    <td>{{form.op_dept}}</td>
    <td>{{form.tid}}</td>
  </tr>
  <tr>
    <td>{{form.job_content}}</td>
    <td id="content-date-wrappers">{{form.job_hours}}</td>
    <td>{{form.relation_code}}</td>
  </tr>
  <tr>
    <td class="default-textarea">{{form.requirement}}</td>
    <td colspan="2" class="default-textarea">{{form.next_step}}</td>
  </tr>
</table>
{{form.actions}}